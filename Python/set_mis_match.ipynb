    "class set_mismatch:\n",
    "    def findErrorNums(nums):\n",
    "        missingNum, doubledNum = -1, -1\n",
    "\n",
    "        for i in range(1, len(nums)+1):\n",
    "            numOccurences = nums.count(i)\n",
    "            if numOccurences == 2:\n",
    "                doubledNum = i\n",
    "            elif numOccurences == 0:\n",
    "                missingNum = i\n",
    "\n",
    "        return [doubledNum, missingNum]"
